<%= form_with model: @work do |form| %>
  <div class="form-groop">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="form-groop">
    <%= form.label :body %>
    <%= form.text_field :body %>
  </div>
  <div class="form-groop">
    <%= form.label :info %>
    <%= form.text_field :info %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= button_to :return, works_path, method: :get%>
